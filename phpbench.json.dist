{
  "bootstrap": "benchmark/vendor/autoload.php",
  "path": "benchmark/phpbench",
  "retry_threshold": 4,
  "reports": {
    "table": {
      "generator": "table",
      "cols": [
        "benchmark",
        "subject",
        "mem_peak",
        "mean",
        "best",
        "diff"
      ],
      "sort": {
        "mean": "asc"
      }
    }
  },
  "php_config": {
    "extension": [ "modules/simdjson.so" ]
  }
}
